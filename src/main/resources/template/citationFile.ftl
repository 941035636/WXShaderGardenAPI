                        <w:highlight w:val="none"/>
                        <w:lang w:val="EN-US" w:fareast="ZH-CN"/>
                    </w:rPr>
                    <w:t> 重物打击</w:t>
                </w:r>
            </w:p>
            <w:p>
                <w:pPr>
                    <w:spacing w:line="360" w:line-rule="auto"/>
                    <w:ind w:first-line="560" w:first-line-chars="200"/>
                    <w:jc w:val="left"/>
